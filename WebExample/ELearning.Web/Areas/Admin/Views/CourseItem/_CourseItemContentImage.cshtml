@model IEnumerable<ELearning.ViewModels.Common.BusinessImageVM>

<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-md-55">
            <div class="thumbnail">
                <div class="image view view-first">
                    <img style="width: 100%; display: block;" src="@item.ImageFilePath." alt="image" />
                    <div class="mask no-caption">
                        <div class="tools tools-bottom">
                            <a href="#"><i class="fa fa-link"></i></a>
                            <a href="#"><i class="fa fa-pencil"></i></a>
                            <a href="#"><i class="fa fa-times"></i></a>
                        </div>
                    </div>
                </div>
                <div class="caption">
                    <p><strong>图片名称</strong>
                </div>
            </div>
        </div>
    }
</div>